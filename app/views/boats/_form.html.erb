<h2>Add a new boat</h2>
<%= simple_form_for(@boat) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :category %>
  <%= f.text_field :category %>

  <%= f.label :capacity %>
  <%= f.text_field :capacity %>

  <%= f.association :city %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.button :submit %>
<% end %>
