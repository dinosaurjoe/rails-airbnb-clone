
<%= simple_form_for(@user) do |f| %>

  <%= f.input :profile_img %>
  <%= f.input :profile_img_cache, as: :hidden %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :bio %>
  <%= f.text_field :bio %>

  <%= f.button :submit %>
<% end %>
